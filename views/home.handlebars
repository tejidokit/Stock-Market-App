
{{posted_stuff}}

<br/>

<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">{{ stock.companyName }} ({{ stock.symbol }})</h1>
    <br/>
        <strong> Latest Price: </strong>${{stock.latestPrice}}
        <br/>
        <strong> Previous Close: </strong>${{stock.previousClose}}
        <br/>
        <strong> Percent Change: </strong>{{stock.changePercent}}%
        <br/>
        <strong> Market Cap: </strong>${{stock.marketCap}}
        <br/>
        <strong> Year to Date Change: </strong>{{stock.ytdChange}}%
        <br/>
        <strong> 52 Week High: </strong>{{stock.week52High}}
        <br/>
        <strong> 52 Week Low: </strong>{{stock.week52Low}}
        <br/>
  </div>
</div>



